<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <Description>The tool to display the sql generated by EntityFrameworkCore, developers can view the sql and the excute time via browser.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/188867052/EntityFrameworkCore.SqlProfile</PackageProjectUrl>
    <PackageReleaseNotes>Microsoft.AspNetCore.Mvc" Version="2.1.0"
Microsoft.EntityFrameworkCore" Version="2.1.0"
Microsoft.Extensions.Logging" Version="2.1.0"

Support Sqlite, SqlServer</PackageReleaseNotes>
    <AssemblyVersion>2.1.0.0</AssemblyVersion>
    <FileVersion>2.1.0.0</FileVersion>
    <Version>2.1.0</Version>
    <RepositoryUrl>https://github.com/188867052/EntityFrameworkCore.SqlProfile</RepositoryUrl>
    <RepositoryType>https://github.com</RepositoryType>
    <PackageTags>EntityFrameworkCore SqlProfile SqlServer Api Sqlite ORM AspNetCore</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.1.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.1.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="2.1.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
